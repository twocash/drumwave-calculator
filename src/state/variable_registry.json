  "naming_collisions": [
    {
      "issue": "Adoption Rate Inconsistency",
      "variables_affected": ["dwalletAdoption", "retailer_adoptionRate"],
      "description": "Standalone Model uses 'dwalletAdoption' (a component of effectiveOptIn), while Network Effects config shows 'Adoption Rate (%)' as the COMBINED metric (dwalletAdoption × activeConsent). This creates confusion.",
      "canonical_suggestion": "Use 'effectiveOptIn' or 'participationRate' consistently across all tabs. Display it as 'Adoption Rate' in UI."
    },
    {
      "issue": "Transaction Terminology",
      "variables_affected": ["annualTransactions"],
      "description": "Variable is named 'annualTransactions' but labeled as 'Transactions/Year' - semantically identical but inconsistent naming pattern.",
      "canonical_suggestion": "Standardize on either 'annualTransactions' or 'transactionsPerYear' throughout codebase."
    },
    {
